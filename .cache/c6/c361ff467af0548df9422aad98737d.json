age responsive */\n.contenedor img {\n  width: 100%;\n  height: auto;\n}\n\n.contenedor header {\n  grid-area: header;\n}\n\n.contenedor .destacado{\n  grid-area: destacado;\n  /* Container needed to position the button.*/\n  position: relative;\n  width: 100%;\n}\n\n/* Centered text */\n.contenedor .centered {\n  position: absolute;\n  top: 40%;\n  left: 50%;\n  transform: translate(-50%, -50%);\n  font-size: 30px;\n  font-weight: bold;\n}\n\n/* Style the button and place it in the middle of the container/image */\n.contenedor .btn {\n  position: absolute;\n  top: 80%;\n  left: 50%;\n  transform: translate(-50%, -50%);\n  -ms-transform: translate(-50%, -50%);\n  background-color: #555;\n  color: white;\n  font-size: 14px;\n  font-weight: bold;\n  padding: 10px 20px;\n  border: none;\n  cursor: pointer;\n  border-radius: 20px;\n}\n\n.contenedor .btn:hover {\n  background-color: black;\n}\n\n.contenedor .delmes {\n  grid-area: delmes;\n  padding: 3%;\n}\n\n.contenedor .delmes1 {\n  grid-area: delmes1;\n  text-align:center;\n  margin:10% auto;\n  display:flex;\n  align-items: center\n}\n\n.contenedor .delmes1 img{\n  width: 70%;\n  height: auto;\n}\n\n.contenedor .delmes2 {\n  grid-area: delmes2;\n  margin:10%;\n  text-align:justify;\n}\n\n.contenedor .descripcion {\n  grid-area: descripcion;\n  margin-left: 10%;\n  margin-right: 10%;\n  text-align:justify;\n}\n\n.contenedor .relacionados {\n  grid-area: relacionados;\n  padding: 3%;\n}\n\n.contenedor .relacionados1 {\n  grid-area: relacionados1;\n}\n.contenedor .relacionados2 {\n  grid-area: relacionados2;\n}\n.contenedor .relacionados3 {\n  grid-area: relacionados3;\n}\n.contenedor .relacionados4 {\n  grid-area: relacionados4;\n}\n\n.contenedor .footer{\n  grid-area: footer;\n  padding: 3%;\n  margin-top:3%;\n  font-size: 14px;\n  text-align: center;\n  background-color: rgb(80, 80, 80);\n  color: #fff;\n}\n\n.autor {\n  color: red;\n  font-size: 16px;\n  padding-bottom: 5%;\n}\n\n.titulo {\n  font-size: 26px;\n  padding-bottom: 5%;\n}\n\n@media screen and (max-width: 830px){\n  .contenedor {\n    grid-template-areas:\"header header header header\"\n                        \"destacado destacado destacado destacado\"\n                        \"delmes delmes delmes delmes\"\n                        \"delmes1 delmes1 delmes2 delmes2\"\n                        \"descripcion descripcion descripcion descripcion\"\n                        \"relacionados relacionados relacionados relacionados\"\n                        \"relacionados1 relacionados1 relacionados2 relacionados2\"\n                        \"relacionados3 relacionados3 relacionados4 relacionados4\"\n                        \"footer footer footer footer\";\n  }\n\n}\n\n@media screen and (max-width: 630px){\n  /* Centered text */\n  .contenedor .centered {\n    position: absolute;\n    top: 40%;\n    left: 50%;\n    transform: translate(-50%, -50%);\n    font-size: 21px;\n    font-weight: bold;\n  }\n  /* Style the button and place it in the middle of the container/image */\n.contenedor .btn {\n  position: absolute;\n  top: 80%;\n  left: 50%;\n  transform: translate(-50%, -50%);\n  -ms-transform: translate(-50%, -50%);\n  background-color: #555;\n  color: white;\n  font-size: 11px;\n  font-weight: bold;\n  padding: 10px 20px;\n  border: none;\n  cursor: pointer;\n  border-radius: 20px;\n  }\n}\n\n@media screen and (max-width: 430px){\n  .contenedor {\n    grid-template-areas:\"header header header header\"\n                        \"destacado destacado destacado destacado\"\n                        \"delmes delmes delmes delmes\"\n                        \"delmes1 delmes1 delmes1 delmes1\"\n                        \"delmes2 delmes2 delmes2 delmes2\"\n                        \"descripcion descripcion descripcion descripcion\"\n                        \"relacionados relacionados relacionados relacionados\"\n                        \"relacionados1 relacionados1 relacionados1 relacionados1\"\n                        \"relacionados2 relacionados2 relacionados2 relacionados2\"\n                        \"relacionados3 relacionados3 relacionados3 relacionados3\"\n                        \"relacionados4 relacionados4 relacionados4 relacionados4\"\n                        \"footer footer footer footer\";\n  }\n  /* Centered text */\n  .contenedor .centered {\n    position: absolute;\n    top: 40%;\n    left: 50%;\n    transform: translate(-50%, -50%);\n    font-size: 13px;\n    font-weight: bold;\n  }\n  /* Style the button and place it in the middle of the container/image */\n.contenedor .btn {\n  position: absolute;\n  top: 80%;\n  left: 50%;\n  transform: translate(-50%, -50%);\n  -ms-transform: translate(-50%, -50%);\n  background-color: #555;\n  color: white;\n  font-size: 8px;\n  font-weight: bold;\n  padding: 5px 10px;\n  border: none;\n  cursor: pointer;\n  border-radius: 10px;\n  }\n}\n\n/* CSS menu */\n\nheader {\n  width: 100%;\n  height: 100px;\n  display: flex;\n  align-items: center;\n  justify-content: space-between;\n  background: white;\n  color: black;\n}\n\n/*.logo {\n  justify-content: space-around;\n}*/\n\n.nav {\n  margin-right: 15%;\n  display: flex;\n  justify-content: space-around;\n  width: 30%;\n}\n\n.navlink {  \n  list-style: none;\n  margin: 0.50em;\n  letter-spacing: 2px;\n}\n\n.navlink a {\n  color: black;\n  text-decoration: none;\n  font-size: 1.2em;\n}\n\n.burger {\n  font-size: 3em;\n  display: none;\n}\n\n@media screen and (max-width: 830px) {\n  .burger {\n      display: block;\n  }\n  .nav {\n      z-index:1;\n      margin-right: 5%;\n      background: white;\n      position: absolute;\n      right: -100%;\n      top: 145px;\n      width: 40%;\n      height: 50%;\n      flex-direction: column;\n      justify-content: space-around;\n      padding: 0;\n      transition: all 400ms;\n  }\n  .navlink {\n      text-align: center;\n  }\n\n  .nav-active {\n      right: 0;\n  }\n}\n\n"},"lineCount":null}},"error":null,"hash":"05e8b8df7aa4670651230e1638d50477","cach
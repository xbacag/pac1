of the container/image */\n.contenedor .btn {\n  position: absolute;\n  top: 80%;\n  left: 50%;\n  transform: translate(-50%, -50%);\n  -ms-transform: translate(-50%, -50%);\n  background-color: #555;\n  color: white;\n  font-size: 14px;\n  font-weight: bold;\n  padding: 10px 20px;\n  border: none;\n  cursor: pointer;\n  border-radius: 20px;\n}\n\n.contenedor .btn:hover {\n  background-color: black;\n}\n\n.contenedor .ultimos {\n  grid-area: ultimos;\n  padding: 3%;\n}\n\n.contenedor .ultimos1 {\n  grid-area: ultimos1;\n}\n\n.contenedor .ultimos2 {\n  grid-area: ultimos2;\n}\n\n.contenedor .ultimos3 {\n  grid-area: ultimos3;\n}\n\n.contenedor .ultimos4 {\n  grid-area: ultimos4;\n}\n\n.contenedor .delmes {\n  grid-area: delmes;\n  padding: 3%;\n}\n\n.contenedor .delmes1 {\n  grid-area: delmes1;\n  text-align:center;\n  margin:10% auto;\n  display:flex;\n  align-items: center\n}\n\n.contenedor .delmes1 img{\n  width: 70%;\n  height: auto;\n}\n\n.contenedor .delmes2 {\n  grid-area: delmes2;\n  margin:10%;\n  text-align:justify;\n}\n\n.contenedor .footer{\n  grid-area: footer;\n  padding: 3%;\n  font-size: 14px;\n  text-align: center;\n  background-color: darkgray;\n  color: #fff;\n}\n\n.autor {\n  color: red;\n  font-size: 16px;\n  padding-bottom: 5%;\n}\n\n.titulo {\n  font-size: 26px;\n  padding-bottom: 5%;\n}\n\n@media screen and (max-width: 830px){\n  .contenedor {\n    grid-template-areas:\"header header header header\"\n                        \"destacado destacado destacado destacado\"\n                        \"ultimos ultimos ultimos ultimos\"\n                        \"ultimos1 ultimos1 ultimos2 ultimos2\"\n                        \"ultimos3 ultimos3 ultimos4 ultimos4\"\n                        \"delmes delmes delmes delmes\"\n                        \"delmes1 delmes1 delmes2 delmes2\"\n                        \"footer footer footer footer\";\n  }\n\n}\n\n@media screen and (max-width: 630px){\n  /* Centered text */\n  .contenedor .centered {\n    position: absolute;\n    top: 40%;\n    left: 50%;\n    transform: translate(-50%, -50%);\n    font-size: 21px;\n    font-weight: bold;\n  }\n  /* Style the button and place it in the middle of the container/image */\n.contenedor .btn {\n  position: absolute;\n  top: 80%;\n  left: 50%;\n  transform: translate(-50%, -50%);\n  -ms-transform: translate(-50%, -50%);\n  background-color: #555;\n  color: white;\n  font-size: 11px;\n  font-weight: bold;\n  padding: 10px 20px;\n  border: none;\n  cursor: pointer;\n  border-radius: 20px;\n  }\n}\n\n@media screen and (max-width: 430px){\n  .contenedor {\n    grid-template-areas:\"header header header header\"\n                        \"destacado destacado destacado destacado\"\n                        \"ultimos ultimos ultimos ultimos\"\n                        \"ultimos1 ultimos1 ultimos1 ultimos1\"\n                        \"ultimos2 ultimos2 ultimos2 ultimos2\"\n                        \"ultimos3 ultimos3 ultimos3 ultimos3\"\n                        \"ultimos4 ultimos4 ultimos4 ultimos4\"\n                        \"delmes delmes delmes delmes\"\n                        \"delmes1 delmes1 delmes1 delmes1\"\n                        \"delmes2 delmes2 delmes2 delmes2\"\n                        \"footer footer footer footer\";\n  }\n  /* Centered text */\n  .contenedor .centered {\n    position: absolute;\n    top: 40%;\n    left: 50%;\n    transform: translate(-50%, -50%);\n    font-size: 13px;\n    font-weight: bold;\n  }\n  /* Style the button and place it in the middle of the container/image */\n.contenedor .btn {\n  position: absolute;\n  top: 80%;\n  left: 50%;\n  transform: translate(-50%, -50%);\n  -ms-transform: translate(-50%, -50%);\n  background-color: #555;\n  color: white;\n  font-size: 8px;\n  font-weight: bold;\n  padding: 5px 10px;\n  border: none;\n  cursor: pointer;\n  border-radius: 10px;\n  }\n}\n\n/* CSS menu */\n\nheader {\n  width: 100%;\n  height: 100px;\n  display: flex;\n  align-items: center;\n  justify-content: space-between;\n  background: white;\n  color: grey;\n}\n\n/*.logo {\n  justify-content: space-around;\n}*/\n\n.nav {\n  margin-right: 15%;\n  display: flex;\n  justify-content: space-around;\n  width: 30%;\n}\n\n.navlink {  \n  list-style: none;\n  margin: 0.50em;\n  letter-spacing: 2px;\n}\n\n.navlink a {\n  color: grey;\n  text-decoration: none;\n  font-size: 1.2em;\n}\n\n.burger {\n  font-size: 3em;\n  display: none;\n}\n\n@media screen and (max-width: 830px) {\n  .burger {\n      display: block;\n  }\n  .nav {\n      z-index:1;\n      margin-right: 5%;\n      background: white;\n      position: absolute;\n      right: -100%;\n      top: 145px;\n      width: 40%;\n      height: calc(100% - 70px);\n      flex-direction: column;\n      justify-content: space-around;\n      padding: 0;\n      transition: all 400ms;\n  }\n  .navlink {\n      text-align: center;\n  }\n\n  .nav-active {\n      right: 0;\n  }\n}\n\n"},"lineCount":null}},"error":null,"hash":"9b97696672066e8feeffce9c7955aa77","cacheData":{"env":{}}}
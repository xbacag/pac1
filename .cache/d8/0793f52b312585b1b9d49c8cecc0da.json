nedor .nav {\n  grid-area: nav;\n}\n\n.contenedor .destacado{\n  grid-area: destacado;\n  /* Container needed to position the button.*/\n  position: relative;\n  width: 100%;\n}\n\n/* Centered text */\n.contenedor .centered {\n  position: absolute;\n  top: 40%;\n  left: 50%;\n  transform: translate(-50%, -50%);\n  font-size: 30px;\n  font-weight: bold;\n}\n\n/* Style the button and place it in the middle of the container/image */\n.contenedor .btn {\n  position: absolute;\n  top: 80%;\n  left: 50%;\n  transform: translate(-50%, -50%);\n  -ms-transform: translate(-50%, -50%);\n  background-color: #555;\n  color: white;\n  font-size: 14px;\n  font-weight: bold;\n  padding: 10px 20px;\n  border: none;\n  cursor: pointer;\n  border-radius: 20px;\n}\n\n.contenedor .btn:hover {\n  background-color: black;\n}\n\n.contenedor .ultimos {\n  grid-area: ultimos;\n  padding: 3%;\n}\n\n.contenedor .ultimos1 {\n  grid-area: ultimos1;\n}\n\n.contenedor .ultimos2 {\n  grid-area: ultimos2;\n}\n\n.contenedor .ultimos3 {\n  grid-area: ultimos3;\n}\n\n.contenedor .ultimos4 {\n  grid-area: ultimos4;\n}\n\n.contenedor .categorias {\n  grid-area: categorias;\n  padding: 3%;\n}\n\n.contenedor .categorias1 {\n  grid-area: categorias1;\n}\n\n.contenedor .categorias2 {\n  grid-area: categorias2;\n}\n\n.contenedor .categorias3 {\n  grid-area: categorias3;\n}\n\n.contenedor .categorias4 {\n  grid-area: categorias4;\n}\n\n.contenedor .delmes {\n  grid-area: delmes;\n  padding: 3%;\n}\n\n.contenedor .delmes1 {\n  grid-area: delmes1;\n  text-align:center;\n  margin:10% auto;\n  display:flex;\n  align-items: center\n}\n\n.contenedor .delmes1 img{\n  width: 70%;\n  height: auto;\n}\n\n.contenedor .delmes2 {\n  grid-area: delmes2;\n  margin:10%;\n  text-align:left;\n}\n\n.contenedor .footer{\n  grid-area: footer;\n  padding: 3%;\n  font-size: 14px;\n  text-align: center;\n  background-color: darkgray;\n  color: #fff;\n}\n\n.autor {\n  color: red;\n  font-size: 16px;\n  padding-bottom: 5%;\n}\n\n.titul